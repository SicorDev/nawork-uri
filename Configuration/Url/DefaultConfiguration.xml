        <!-- polisch diactric characters :: begin -->
        <char from="Ą" to="A"/>
        <char from="Ć" to="C"/>
        <char from="Ę" to="E"/>
        <char from="Ł" to="L"/>
        <char from="Ń" to="N"/>
        <char from="Ó" to="O"/>
        <char from="Ś" to="S"/>
