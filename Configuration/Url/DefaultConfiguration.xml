<?xml version="1.0" encoding="utf-8"?>
<nawork_uri>

    <append>/</append>
    <castTypeToInt>1</castTypeToInt>
    <castLToInt>1</castLToInt>
    <redirectOnParameterDiff>1</redirectOnParameterDiff>
    <redirectStatus>301</redirectStatus>

    <pagenotfound>
        <status>HTTP/1.0 404 Not Found</status>
        <!--
        <status domain="www.domain.tld">HTTP/1.0 404 Not Found</status>
        <behavior type="message"><![CDATA[ <h1>Page not found!</h1> ]]></behavior>
        <behavior type="redirect">http://www.domain.tld/?id=notfound</behavior>
        <behavior type="page">http://www.domain.tld/?id=notfound</behavior>
        <behavior type="page" domain="www.domain.tld">http://www.domain.tld/index.php?id=notfound</behavior>
        -->
        <behavior type="message"><![CDATA[ <h1>Page not found!</h1> ]]></behavior>
    </pagenotfound>

    <pageNotAccessible>
        <status>HTTP/1.0 403 Forbidden</status>
        <behavior type="message">
            <![CDATA[<html><head><title>403 Forbidden</title></head><body><h1>Forbidden!</h1><p>You don't have the permission to access the requested page</p></body></html>]]></behavior>
    </pageNotAccessible>

    <transliteration>
        <!--
        <char from="ä" to="ae" />
        <char from="ö" to="oe" />
        <char from="ü" to="ue" />
        <char from="Ä" to="ae" />
        <char from="Ö" to="oe" />
        <char from="Ü" to="ue" />
        -->
        <!-- cyrillic (russian) transliterations, not supported by glibc :: begin-->
        <char from="а" to="a"/>
        <char from="б" to="b"/>
        <char from="в" to="v"/>
        <char from="г" to="g"/>
        <char from="д" to="d"/>
        <char from="е" to="e"/>
        <char from="ж" to="z"/>
        <char from="з" to="z"/>
        <char from="и" to="i"/>
        <char from="й" to="j"/>
        <char from="к" to="k"/>
        <char from="л" to="l"/>
        <char from="м" to="m"/>
        <char from="н" to="n"/>
        <char from="о" to="o"/>
        <char from="п" to="p"/>
        <char from="р" to="r"/>
        <char from="с" to="s"/>
        <char from="т" to="t"/>
        <char from="у" to="u"/>
        <char from="ф" to="f"/>
        <char from="х" to="ch"/>
        <char from="ц" to="c"/>
        <char from="ч" to="c"/>
        <char from="ш" to="s"/>
        <char from="щ" to="s"/>
        <char from="э" to="e"/>
        <char from="ю" to="ju"/>
        <char from="я" to="ja"/>

        <char from="А" to="a"/>
        <char from="Б" to="b"/>
        <char from="В" to="v"/>
        <char from="Г" to="g"/>
        <char from="Д" to="d"/>
        <char from="Е" to="e"/>
        <char from="Ж" to="z"/>
        <char from="З" to="z"/>
        <char from="И" to="i"/>
        <char from="Й" to="j"/>
        <char from="К" to="k"/>
        <char from="Л" to="l"/>
        <char from="М" to="m"/>
        <char from="Н" to="n"/>
        <char from="О" to="o"/>
        <char from="П" to="p"/>
        <char from="Р" to="r"/>
        <char from="С" to="s"/>
        <char from="Т" to="r"/>
        <char from="У" to="u"/>
        <char from="Ф" to="f"/>
        <char from="Х" to="z"/>
