    <parameters>
        <parameterConfiguration>
            <name>cHash</name>
            <type>Hidden</type>
        </parameterConfiguration>
        <parameterConfiguration>
            <name>no_cache</name>
            <type>Hidden</type>
        </parameterConfiguration>
        <parameterConfiguration>
            <name>L</name>
            <type>ValueMap</type>
            <map>
                <mapping>
                    <value>0</value>
                    <replacement></replacement>
                </mapping>
                <mapping>
                    <value>1</value>
                    <replacement>en</replacement>
                </mapping>
            </map>
        </parameterConfiguration>
        <parameterConfiguration>
            <name>id</name>
            <type>PagePath</type>
            <field>alias//nav_title//title</field>
        </parameterConfiguration>
        <parameterConfiguration>
            <name>tx_ttnews[tt_news]</name>
            <type>Database</type>
            <table>test_news</table>
            <replacement>{title}</replacement>
        </parameterConfiguration>
    </parameters>
</nawork_uri>
